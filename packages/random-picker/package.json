{
  "name": "@mxssfd/random-picker",
  "version": "5.0.0-beta.9",
  "description": "从数组中随机挑选出n个选项的js工具类库",
  "main": "index.js",
  "module": "dist/random-picker.esm-bundler.js",
  "exports": {
    ".": {
      "import": "./dist/random-picker.esm-bundler.js",
      "require": "./index.js"
    },
    "./": "./dist"
  },
  "types": "dist/random-picker.d.ts",
  "keywords": [
    "random",
    "choose",
    "array",
    "select",
    "items",
    "sequential",
    "unique",
    "pick-some",
    "pick",
    "picker",
    "take",
    "weights"
  ],
  "files": [
    "index.js",
    "index.mjs",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/ts-utils.git"
  },
  "buildOptions": {
    "name": "tsUtilsRandomPicker",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "scripts": {
    "apie": "api-extractor run"
  },
  "dependencies": {
    "@mxssfd/types": "5.0.0-beta.9"
  },
  "author": "dyh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/ts-utils/issues"
  },
  "homepage": "https://github.com/mengxinssfd/ts-utils/tree/master/packages/random-picker"
}
