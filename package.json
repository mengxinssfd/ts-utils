{
  "name": "@mxssfd/ts-utils",
  "version": "1.1.7",
  "description": "ts工具函数",
  "main": "./lib-umd/index.js",
  "module": "./lib-es/index.js",
  "jsnext:min": "./lib-es/index.js",
  "typings": "./types/index.d.ts",
  "files": [
    "src",
    "lib-es/*",
    "lib-umd/*",
    "types/*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib-* types",
    "build": "run-p clean lib:es lib:umd",
    "lib:umd": "set NODE_ENV=production&webpack --mode production",
    "lib:es": "tsc --build tsconfig.es.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pub": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/utilTs.git"
  },
  "keywords": [
    "ts",
    "typscript",
    "js",
    "javascript",
    "utils",
    "工具"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.3.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
