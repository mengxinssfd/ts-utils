{
  "private": true,
  "version": "5.0.0",
  "description": "utils工具函数ts/js库",
  "jsdelivr": "./lib-umd/index.js",
  "main": "./lib-cjs/index.js",
  "module": "./lib-es/index.js",
  "umd:main": "./lib-umd/index.js",
  "jsnext:min": "./lib-es/index.js",
  "typings": "./types/",
  "sideEffects": false,
  "files": [
    "lib-es/*",
    "lib-umd/*",
    "lib-cjs/*",
    "types/*"
  ],
  "scripts": {
    "ru": "rollup -c --noConflict",
    "clean": "rimraf lib-* types coverage",
    "build-all": "run-p lib:es lib:cjs ru",
    "build": "run-s clean build-all copy-d-all",
    "build-que": "cmd-que -cmd=\"npm run clean,npm run lib:es,npm run ru,npm run copy-d\"",
    "lib:umd": "set NODE_ENV=production&webpack --mode production",
    "lib:es": "tsc --build tsconfig.es.json",
    "lib:cjs": "tsc --build tsconfig.cjs.json",
    "pub": "npm publish --access=public",
    "copy-d-all": "run-p copy-types-a",
    "copy-types-a": "cp -R packages/types types",
    "copy-types": "copy -R packages/types types",
    "rename": "reName lib-umd\\index.umd.d.ts index.d.ts",
    "test": "jest",
    "cov": "jest --coverage",
    "cov-no-cache": "jest --coverage --no-cache",
    "dev": "set NODE_ENV=development&webpack-dev-server --config webpack.dev.config.js --host 0.0.0.0 --open \"http://localhost:8080/touch.html\"",
    "clear_jest": "jest --clearCache",
    "lint": "eslint packages",
    "prettier-check": "prettier packages __test__ --check",
    "lint-check": "run-p lint prettier-check",
    "build_new": "node scripts/build.js -t",
    "ts-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/ts-utils.git"
  },
  "keywords": [
    "ts",
    "typscript",
    "js",
    "javascript",
    "utils",
    "工具"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@microsoft/api-extractor": "7.20.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^16.11.42",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "chalk": "^4.1.2",
    "core-js": "^3.23.3",
    "enquirer": "^2.3.6",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-polyfill-node": "^0.9.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "author": "dyh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/ts-utils/issues"
  },
  "homepage": "https://github.com/mengxinssfd/ts-utils#readme",
  "exports": {
    ".": {
      "import": "./lib-es/index.js",
      "require": "./lib-cjs/index.js"
    },
    "./": "./"
  }
}
