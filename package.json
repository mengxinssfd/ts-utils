{
  "name": "@mxssfd/ts-utils",
  "version": "3.0.5",
  "description": "utils工具函数ts/js库",
  "main": "./lib-umd/index.js",
  "module": "./lib-es/index.js",
  "jsnext:min": "./lib-es/index.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib-es/*",
    "lib-umd/*",
    "lib-cjs/*",
    "types/*"
  ],
  "scripts": {
    "ru": "rollup -c --noConflict",
    "clean": "rimraf lib-* types coverage",
    "build-all": "run-p lib:es lib:cjs ru",
    "build": "run-s clean build-all copy-d",
    "build-que": "cmd-que -cmd=\"npm run clean,npm run lib:es,npm run ru,npm run copy-d\"",
    "lib:umd": "set NODE_ENV=production&webpack --mode production",
    "lib:es": "tsc --build tsconfig.es.json",
    "lib:cjs": "tsc --build tsconfig.cjs.json",
    "pub": "npm publish --access=public",
    "copy-d": "copy src\\*.d.ts types\\",
    "rename": "reName lib-umd\\index.umd.d.ts index.d.ts",
    "test": "jest",
    "cov": "jest --coverage",
    "cov-no-cache": "jest --coverage --no-cache",
    "dev": "set NODE_ENV=development&webpack-dev-server --config webpack.dev.config.js --host 0.0.0.0 --open \"http://localhost:8080/touch\"",
    "clear_jest": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/ts-utils.git"
  },
  "keywords": [
    "ts",
    "typscript",
    "js",
    "javascript",
    "utils",
    "工具"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.6.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^24.0.24",
    "@types/node": "^14.14.31",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.3.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^2.45.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "author": "dyh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mengxinssfd/ts-utils/issues"
  },
  "homepage": "https://github.com/mengxinssfd/ts-utils#readme"
}
